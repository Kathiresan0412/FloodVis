MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-host>/<db-name>?retryWrites=true&w=majority
PORT=4000

# Secret key used for signing JWTs for login
JWT_SECRET=change_this_to_a_long_random_string

# OpenWeatherMap API key (for weather / dashboard data)
OPENWEATHERMAP_API_KEY=your_api_key_here

# Twilio (for sending SOS alerts by SMS to emergency contacts)
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890

